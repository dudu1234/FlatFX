@model FlatFXWebClient.ViewModels.CompanyUserAllEntitiesModelView

<div class="form-group">
    @if (Model.ShowContactDetailsForUser)
    {
        @Html.TextBoxFor(m => m.UserVM.userContactDetails.contactDetailsEx.Address, new { @class = "form-control", @placeholder = Html.DisplayNameFor(x => x.UserVM.userContactDetails.contactDetailsEx.Address) });
    }
    else
    {
        @Html.TextBoxFor(m => m.companyVM.companyContactDetails.contactDetailsEx.Address, new { @class = "form-control", @placeholder = Html.DisplayNameFor(x => x.companyVM.companyContactDetails.contactDetailsEx.Address) });
