@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Manage User";
}

<div class="row user-manage">
    <div class="text-center user-manage-header">User Manage</div>
    <div class="col-xs-8 col-xs-offset-4">
        <p class="text-success">@ViewBag.StatusMessage</p>
        <p class="text-success">@TempData["Result"]</p>
        <p class="error">@ViewBag.ErrorMessage</p>
        <p class="text-success">@ViewBag.Result</p>
    </div>
</div>
<div class="row user-manage" style="background-color: #b2eaef">
    <div class="col-xs-3">
        <div class="sidebar-nav">
            <div class="navbar navbar-default" role="navigation">
                <div class="navbar-collapse collapse sidebar-navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li class="active">@Html.ActionLink("Main", "UserIndex", "Manage")</li>
                        <li>@Html.ActionLink("User Details", "EditByUser", "ApplicationUsers")</li>
                        <li>@Html.ActionLink("Company Details", "IndexUser", "Companies")</li>
                        <li>@Html.ActionLink("Company Account Details", "IndexUser", "CompanyAccounts")</li>
                        <li>@Html.ActionLink("Bank/Provider Accounts Details", "IndexUser", "ProviderAccounts")</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-9">
        @RenderBody()
    </div>
</div>
