    <link rel="stylesheet" href="http://kendo.cdn.telerik.com/2013.1.319/styles/kendo.bootstrap.min.css" />
    <link href="http://kendo.cdn.telerik.com/2013.1.319/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="http://kendo.cdn.telerik.com/2013.1.319/styles/kendo.default.min.css" rel="stylesheet" />
    <link href="http://kendo.cdn.telerik.com/2013.1.319/styles/kendo.rtl.min.css" rel="stylesheet" />
    <script src="http://kendo.cdn.telerik.com/2013.1.319/js/kendo.all.min.js"></script>
    <script src="http://kendo.cdn.telerik.com/2013.1.319/js/kendo.aspnetmvc.min.js"></script>
    <script src="~/JS/FFx.js"></script>
</head>
